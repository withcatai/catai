{
  "name": "catai",
  "version": "1.0.0",
  "description": "Chat UI for the Alpaca models",
  "main": "dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "bin": {
    "catai": "./dist/cli/cli.js"
  },
  "files": [
    "dist/",
    "llama/",
    "llamaBins/",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "postinstall": "#node ./dist/cli/cli.js postinstall",
    "prebuild": "rm -rf ./dist ./tsconfig.tsbuildinfo",
    "build": "tsc --build tsconfig.json --force",
    "build:front": "zx scripts/build.js",
    "watch": "tsc --build tsconfig.json --watch --force",
    "test": "npm run test:typescript && npm run lint:eslint",
    "test:typescript": "tsc --build tsconfig.json --dry --force",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint --ext .js --ext .ts ../..",
    "format": "npm run lint:eslint -- --fix",
    "clean": "rm -rf ./node_modules ./dist ./tsconfig.tsbuildinfo",
    "cli": "ts-node-esm -r dotenv/config ./src/cli/cli.ts",
    "new-model": "zx scripts/add-model.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/withcatai/catai.git"
  },
  "keywords": [
    "llama",
    "llama-cpp",
    "bindings",
    "ai",
    "node-gyp",
    "prebuilt-binaries",
    "llm",
    "ggml",
    "ggmlv3",
    "raspberry-pi",
    "self-hosted",
    "local",
    "catai"
  ],
  "author": "Ido S.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/withcatai/catai/issues"
  },
  "homepage": "https://github.com/withcatai/catai#readme",
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cli-progress": "^3.11.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.4.9",
    "@types/object-assign-deep": "^0.4.0",
    "@types/prompts": "^2.4.4",
    "@types/uuid": "^9.0.2",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.46.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-node": "github:giladgd/eslint-plugin-node#dev/giladgd/fixImportExtentionFixingInTypeScript",
    "jsdom": "^22.1.0",
    "semantic-release": "^21.0.7",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.1",
    "typed-emitter": "^2.1.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@tinyhttp/app": "^2.1.4",
    "@tinyhttp/cors": "^2.0.0",
    "ansi-colors": "^4.1.3",
    "body-parser": "^1.20.2",
    "class-pull": "^1.0.2",
    "cli-progress": "^3.12.0",
    "cli-table3": "^0.6.3",
    "commander": "^10.0.0",
    "fs-extra": "^11.1.1",
    "get-port": "^6.1.2",
    "node-llama-cpp": "^1.1.1",
    "object-assign-deep": "^0.4.0",
    "open": "^9.1.0",
    "pretty-bytes": "^6.1.0",
    "pretty-ms": "^8.0.0",
    "prompts": "^2.4.2",
    "sirv": "^2.0.2",
    "turbo-downloader": "^1.3.1",
    "uuid": "^9.0.0",
    "wretch": "^2.6.0",
    "ws": "^8.13.0",
    "yn": "^5.0.0",
    "zx": "^7.2.3"
  }
}
