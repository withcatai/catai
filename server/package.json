{
  "name": "catai",
  "type": "module",
  "version": "0.3.4",
  "description": "Chat UI for the Alpaca models",
  "main": "index.js",
  "engines": {
    "node": ">=16.13.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "build": "zx scripts/package/build.js",
    "install-model": "zx scripts/install.js",
    "download-alpaca-cpp": "zx scripts/download-alpaca-cpp.js",
    "use": "zx scripts/use.js",
    "remove": "zx scripts/remove.js",
    "list": "zx scripts/list.js",
    "models": "zx scripts/models.js",
    "postinstall": "npm run download-alpaca-cpp",
    "prepack": "npm run build && cp ../README.md ./README.md",
    "postpublish": "zx scripts/package/docker-publish.js"
  },
  "keywords": [
    "alpaca",
    "chat",
    "ui",
    "web",
    "catai",
    "chatgpt",
    "openai",
    "llama",
    "LLM"
  ],
  "author": "Ido S.",
  "license": "MIT",
  "dependencies": {
    "@tinyhttp/app": "^2.0.29",
    "@tinyhttp/cors": "^2.0.0",
    "ansi-colors": "^4.1.3",
    "body-parser": "^1.20.2",
    "cli-progress": "^3.12.0",
    "cli-table": "^0.3.11",
    "commander": "^10.0.0",
    "easydl": "^1.0.3",
    "extract-zip": "^2.0.1",
    "fs-extra": "^11.1.1",
    "get-port": "^6.1.2",
    "minimist": "^1.2.8",
    "openurl": "^1.1.1",
    "pretty-bytes": "^6.1.0",
    "pretty-ms": "^8.0.0",
    "sirv": "^2.0.2",
    "try-catch": "^3.0.1",
    "wretch": "^2.5.1",
    "ws": "^8.13.0",
    "yn": "^5.0.0",
    "zx": "^7.2.2"
  },
  "devDependencies": {
    "@types/node": "^18.15.10"
  },
  "peerDependencies": {
    "@llama-node/llama-cpp": "^0.1.0",
    "llama-node": "^0.1.0"
  },
  "bin": {
    "catai": "scripts/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ido-pluto/catai"
  },
  "homepage": "https://github.com/ido-pluto/catai"
